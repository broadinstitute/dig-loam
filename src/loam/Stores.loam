import loamstream.model.Store

final case class MultiPath(
  local: Option[Path],
  google: Option[URI])

final case class MultiStore(
  local: Option[Store],
  google: Option[Store])

final case class Vcf(
  base: Path,
  data: Store,
  tbi: Store)

final case class MultiPathVcf(
  base: MultiPath,
  data: MultiStore,
  tbi: MultiStore)

final case class Plink(
  base: Path,
  data: Seq[Store])

final case class MultiPathPlink(
  base: MultiPath,
  data: Seq[MultiStore])
