val inputFile = path("/user/home/someone/data.vcf")
val outputFile = path("/user/home/someone/dataImputed.vcf")
val phaseCommand = "shapeit"
val imputeCommand = "impute2"

val raw = store[VCF].at(inputFile).asInput
