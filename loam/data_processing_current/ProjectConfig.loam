import scala.util.control.NonFatal

case class ProjectConfig(
  projectId: String)

object ProjectConfig {

  def parse(config: loamstream.conf.DataConfig): ProjectConfig = {

    try {
      new ProjectConfig(config.getStr("projectId"))
    } catch {
      case NonFatal(e) => throw new ConfigValidationException("projectId undefined", e)
    }

  }

}
